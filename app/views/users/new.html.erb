<h1>Sign Up For DemoDaze</h1>

<%= render partial: "/layouts/errors", locals: {object: @user} %>


<%= form_for @user do |f| %>
    <%=f.label :name %>
    <%=f.text_field :name %><br/>
    
    <%=f.label :email %>
    <%=f.email_field :email, required:true %><br/>
    
    <%=f.label :password %>
    <%=f.password_field :password%><br/>

    <%=f.submit "Sign Up" %>
<% end %>